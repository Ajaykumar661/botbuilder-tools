    "@types/globby": "^8.0.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/semver": "^5.5.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "dialogschema": "^1.0.0",
    "mocha": "^5.2.0",
