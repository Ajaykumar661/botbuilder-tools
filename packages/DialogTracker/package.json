  "main": "./lib/dialogTracker.js",
  "types": "./typings/lib/dialogTracker.d.ts",
  "bin": {
    "dialogtracker": "./lib/dialogTracker.js"
  },
  "directories": {
    "bin": "bin",
    "doc": "docs",
    "lib": "lib",
    "typings": "typings",
    "examples": "test/examples"
  },
