<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="Microsoft.Bot.Builder.AI.Luis"
           xmlns:bb="Microsoft.Bot.Builder"
           xmlns:ai="Microsoft.Bot.Builder.AI.Luis"
           xmlns:bl="http://botframework.com/botloader"
           >
    <xs:import namespace="Microsoft.Bot.Builder" schemaLocation="component.xsd"/>
    <xs:element name="LUISRecognizer" bl:implements="bb:IRecognizer, bb:IComponent">
        <xs:complexType>
            <xs:complexContent>
                <!-- Extend the base component definition. -->
                <xs:extension base="bb:component">
                    <xs:attribute name="appId" type="xs:string"/>
                    <xs:attribute name="endpoint" type="xs:string"/>
                    <xs:attribute name="endpointKey" type="xs:string"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
</xs:schema>
